{
  "name": "juice-shop-ctf-cli",
  "version": "4.2.0",
  "description": "Capture-the-Flag (CTF) environment setup tools for OWASP Juice Shop",
  "author": "Bjoern Kimminich <bjoern.kimminich@owasp.org> (https://www.owasp.org/index.php/User:Bjoern_Kimminich)",
  "contributors": [
    "Bjoern Kimminich",
    "Josh Grossman"
  ],
  "keywords": [
    "web security",
    "web application security",
    "webappsec",
    "owasp",
    "pentest",
    "pentesting",
    "security",
    "vulnerable",
    "vulnerability",
    "broken",
    "capture the flag",
    "ctf",
    "ctfd",
    "cli"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkimminich/juice-shop-ctf.git"
  },
  "bugs": {
    "url": "https://github.com/bkimminich/juice-shop-ctf/issues"
  },
  "homepage": "http://owasp-juice.shop",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "juice-shop-ctf": "bin/juice-shop-ctf.js"
  },
  "scripts": {
    "test": "standard && nyc --reporter=html --reporter=text mocha test/unit",
    "e2e": "nyc --reporter=text mocha test/e2e --slow=10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "stryker": "stryker run stryker.conf.js"
  },
  "engines": {
    "node": ">=8 <=10"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "~3.5",
    "colors": "~1.2",
    "dateformat": "~3.0",
    "inquirer": "~5.2.0",
    "joi": "~13.1",
    "js-yaml": "~3.11",
    "jssha": "~2.3",
    "node-zip": "~1.1",
    "path": "~0.12",
    "request": "~2.85",
    "request-promise": "~4.2",
    "yargs": "~11.1.0"
  },
  "devDependencies": {
    "ava": "~0",
    "chai": "~4",
    "chai-as-promised": "~7",
    "chai-spies": "~1",
    "chai-subset": "~1",
    "chai-things": "~0",
    "coveralls": "~3",
    "inquirer-test": "~2",
    "lockfile": "~1",
    "mocha": "~5",
    "nyc": "~11",
    "rewire": "~4.0.0",
    "standard": "~11.0",
    "stryker": "~0",
    "stryker-api": "~0",
    "stryker-html-reporter": "~0",
    "stryker-javascript-mutator": "~0",
    "stryker-mocha-framework": "~0",
    "stryker-mocha-runner": "~0"
  },
  "standard": {
    "env": [
      "node",
      "mocha",
      "chai"
    ],
    "globals": [
      "it",
      "describe",
      "beforeEach"
    ]
  },
  "nyc": {
    "report-dir": "./build/reports/coverage"
  }
}
