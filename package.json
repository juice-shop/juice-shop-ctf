{
  "name": "juice-shop-ctf-cli",
  "version": "1.3.0-SNAPSHOT",
  "description": "Command line client to generate INSERT statements for CTFd with the OWASP Juice Shop challenges",
  "author": "Bjoern Kimminich <bjoern.kimminich@owasp.org> (https://www.owasp.org/index.php/User:Bjoern_Kimminich)",
  "contributors": [
    "Bjoern Kimminich",
    "Josh Grossman"
  ],
  "keywords": [
    "web security",
    "web application security",
    "webappsec",
    "owasp",
    "pentest",
    "pentesting",
    "security",
    "vulnerable",
    "vulnerability",
    "broken",
    "capture the flag",
    "ctf",
    "ctfd",
    "cli"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkimminich/juice-shop-ctf.git"
  },
  "bugs": {
    "url": "https://github.com/bkimminich/juice-shop-ctf/issues"
  },
  "homepage": "https://www.owasp.org/index.php/OWASP_Juice_Shop_Project",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "juice-shop-ctf": "bin/juice-shop-ctf.js"
  },
  "scripts": {
    "test": "standard && nyc --reporter=html --reporter=text mocha test/unit",
    "e2e": "nyc --reporter=text mocha test/e2e --slow=10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "stryker": "stryker run stryker.conf.js"
  },
  "engines": {
    "node": ">=4 <9"
  },
  "dependencies": {
    "bluebird": "~3.4",
    "colors": "~1.1",
    "inquirer": "~3.2",
    "jssha": "~2.3",
    "path": "~0.12",
    "request": "~2.81",
    "request-promise": "~4.2"
  },
  "devDependencies": {
    "ava": "~0",
    "chai": "~4",
    "chai-as-promised": "~7",
    "chai-spies": "~0",
    "coveralls": "~2",
    "inquirer-test": "~1",
    "lockfile": "~1",
    "mocha": "~3",
    "nyc": "~11",
    "rewire": "~2",
    "standard": "~10",
    "stryker": "~0",
    "stryker-api": "~0",
    "stryker-html-reporter": "~0",
    "stryker-mocha-runner": "~0"
  },
  "standard": {
    "env": [
      "node",
      "mocha",
      "chai"
    ],
    "globals": [
      "it",
      "describe",
      "beforeEach"
    ]
  },
  "nyc": {
    "report-dir": "./reports/coverage"
  }
}
